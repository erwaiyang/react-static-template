{
  "name": "react-static-template",
  "version": "0.1.0",
  "description": "One command to get your static HTML file!",
  "main": "index.js",
  "scripts": {
    "start": "webpack-dev-server --config webpack.hot.config.js",
    "clean": "rimraf dist",
    "pack": "webpack --config webpack.build.config.js",
    "build": "npm run clean && npm run pack && node build.js",
    "lint:css": "stylelint \"src/**/*.css\"",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "webpack",
    "webpack3",
    "css-modules",
    "react",
    "hot-reload",
    "static-generator"
  ],
  "author": "Jean Yang",
  "license": "MIT",
  "dependencies": {
    "normalize.css": "^7.0.0",
    "prop-types": "^15.5.10",
    "ramda": "^0.24.1",
    "react": "15.6.1",
    "react-dom": "15.6.1",
    "webpack": "3"
  },
  "devDependencies": {
    "babel-core": "^6.25.0",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.1.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "css-loader": "^0.28.4",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^15.0.1",
    "eslint-config-airbnb-base": "^11.2.0",
    "eslint-plugin-import": "^2.6.0",
    "eslint-plugin-jsx-a11y": "^5.1.0",
    "eslint-plugin-react": "^7.1.0",
    "extract-text-webpack-plugin": "^2.1.2",
    "postcss-loader": "^2.0.6",
    "postcss-modules-values": "^1.3.0",
    "purify-css": "^1.2.5",
    "react-hot-loader": "3.0.0-beta.7",
    "rimraf": "^2.6.1",
    "style-loader": "^0.18.2",
    "stylelint": "^7.12.0",
    "stylelint-config-css-modules": "^1.0.0",
    "stylelint-config-standard": "^16.0.0",
    "webpack-dev-server": "2.5.0",
    "webpack-node-externals": "^1.6.0"
  }
}
